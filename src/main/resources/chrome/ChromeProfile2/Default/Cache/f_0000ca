    <!-- GDPR Script -->
    <script src="//cloudstorage.amari.com/3rd/GDPR/cookieconsent-master/examples/script.js"></script>
    <script src="//cloudstorage.amari.com/3rd/GDPR/cookieconsent-master/src/cookieconsent.js"></script>
    <script>
        $(document).ready(function () {
            (function (C, U) {

                var popupInst;
                var code = 'UK';
                draw(code);

                function draw(code) {
                    var options = getOptions(code);

                    if (popupInst) {
                        popupInst.clearStatus();
                        popupInst.destroy();
                        popupInst = null;
                    }

                    cookieconsent.initialise(options, function (popup) {
                        popupInst = popup;
                        popupInst.autoOpen();

                    }, function (err) {
                        console.error(err)
                    });

                    // show country options on screen (so user knows how this country affected the settings)
                    //document.getElementById('message').innerHTML = getCountryDetails(code);
                }            

                function getOptions(code) {
                    return {
                        type: 'info',
                        regionalLaw: true,
                        law: {
                            // takes the "preferred" options and changes them slightly to match the country's law
                            countryCode: code,
                        },
                        layout: 'basic-close',
                        layouts: {
                            'basic-close': '{{messagelink}}{{close}}',
                        },
                        content: {
                            message: 'We use Cookies - by continuing to use this site or closing this message you\'re agreeing to our',
                            link: 'Cookies policy',
                            href: 'https://' + 'www' + '.amari.com/privacy.aspx',
                            close: '&times;'
                        },
                        cookie: {
                            domain: 'amari.com',
                            expiryDays :30
                        },                       
                        onPopupOpen: function () {
                            repositionTripAdvisorCallout();
                        },
                        onPopupClose: function () {
                            setTimeout(function () { $('.ta-callout').animate({ 'margin-bottom': 0 }); }, 500);
                        }
                    }
                }

                function getCountryDetails(code) {
                    // We only get these to show (on screen) how the country behaves
                    var law = new cookieconsent.Law({});
                    var countryOpts = law.get(code);

                    if (!countryOpts.hasLaw) {
                        return 'Has cookie law? no';
                    }
                    return U.tabularObject({
                        'Has cookie law?': 'yes',
                        'Choice has to be revokable?': countryOpts.revokable ? 'yes' : 'no',
                        'Can be automatically dismissed?': countryOpts.explicitAction ? 'no' : 'yes',
                    });
                }

            }(window.cookieconsent, window.cookieconsent_example_util));

            function repositionTripAdvisorCallout() {
                //$('.ta-callout').css('margin-bottom', $('.cc-window').is(":visible") ? $('.cc-window').outerHeight() + 5 : 0 );
            }
            if (getUrlParameter('utm_source') == 'tripadvisor' && getUrlParameter('promocode') == 'bookdirect') {

                $('.ta-callout').fadeIn();
                $(window).resize(function () {
                    repositionTripAdvisorCallout();
                });
            }
            $('.ta-callout .close').click(function () {
                $('.ta-callout').fadeOut();

                return false;
            });
        });
    </script>
    <!-- /.End GDPR Script -->
