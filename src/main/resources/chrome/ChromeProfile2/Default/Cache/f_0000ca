            'Fri',
            'Sat'];

        var ArrLongDayName = ['Sunday',
            'Monday',
            'Tuesday',
            'Wednesday',
            'Thursday',
            'Friday',
            'Saturday'];


        var today = new Date();
        today = booking.CheckInDate;
        //console.log("-----------------------------------------------");
        //console.log(booking.CheckInDate);
        var tomorrow = new Date();

        tomorrow.setDate(tomorrow.getDate() + 1);
        tomorrow = booking.CheckOutDate;

        var input = document.getElementById('input-id');
        if (!input) {
            input = $('#input-id');
        }

        var inputmask = document.getElementById('input-id-mask');
        if (!inputmask) {
