            <input name="ctl00$ctl00$ucFooter$ucSubscribe$tbMail" type="text" id="tbMail" title="Your email address" class="txtbox" onfocus="(this.value==$(this).attr(&#39;title&#39;))?this.value=&#39;&#39;:this.select()" onblur="(this.value==&#39;&#39;)?this.value=$(this).attr(&#39;title&#39;):&#39;&#39;" placeholder="Your email address" />
