            //search function
            function getSearchOption(elem) {

                var options = {
                    data: booking._setting.PropertyFilter, //  propdest,    //      
                    placeholder: 'Enter a destination or property',
                    getValue: function (element) {

                        if (typeof element.DestinationId == 'undefined') {  //Country
                            return element.CountryName +
                                element.DestinationCountryAllLang +
                                element.CountryNameEN;
                        } else if (typeof element.PropertyId == 'undefined') { //Destination city
                            return element.CityName +
                                element.CityNameAllLang +
                                element.CityNameEN +
                                element.CountryName +
                                element.CountryNameEn +
                                element.CountyNameAllLang;
                        } else { // Property
                            return element.Name +
                                element.NameEN +
                                element.NameAllLang +
                             //   element.Address + element.AddressAllLang +
                                element.DestinationCity + element.DestinationCityAllLang +
                                element.DestinationCountry + element.DestinationCountryAllLang +
                                element.CityName +
                                element.CityNameAllLang +
                                element.CityNameEN +
                                element.CountryName +
                                element.CountryNameEn +
                                element.CountyNameAllLang;
                        }

                    },
                template: {
                    type: "custom",
                    method: function setdestprop(value, item) {

                        //if (typeof item.PropertyId === 'undefined') {
                        //    return "<div style='min-width:320px; color:black;'> <strong><span>" + item + "</span></strong></div>";
                        //} else {
                        //    return "<div style='min-width:320px; color:black;'><span class='tab-space'>" + item.Name + "</span></div>";
                        //}


                        if (typeof item.DestinationId == 'undefined') {  //Country
                            return "<div style='min-width:320px; color:black;'> <strong><span>" + item.CountryName + "</span></strong></div>";
                        } else if (typeof item.PropertyId == 'undefined') { //Destination
                            return "<div style='min-width:320px; color:black;'> <strong><span class='tab-space'>" + item.CityName + "</span></strong></div>";
                        } else { // Property
                            return "<div style='min-width:320px; color:black;'><span class='tab-space'><span class='tab-space'>" + item.Name + "</span></span></div>";
                        }
                     
                       
                    }
                },
                list: {
                    maxNumberOfElements: 200,
                    match: {
                        enabled: true
                    },
                    onSelectItemEvent: function (event) {


                        if (!$.browser.mozilla) {


                            var keyDownEvent = event || window.event,
                                keycode = (keyDownEvent.which) ? keyDownEvent.which : keyDownEvent.keyCode;

                            // console.log(keyCodeNumber);
                            var key_arrow_or_other = elem,
                                LEFT = 37,
                                UP = 38,
                                RIGHT = 39,
                                DOWN = 40;

                            switch (keycode) {

                                case UP:
                                    if (typeof elem.getSelectedItemData().DestinationId == 'undefined') {  //Country
                                        elem.val(elem.getSelectedItemData().CountryName);
                                    } else if (typeof elem.getSelectedItemData().PropertyId == 'undefined') { //Destination
                                        elem.val(elem.getSelectedItemData().CityName);
                                    } else { // Property
                                        elem.val(elem.getSelectedItemData().Name);
                                    }
                                    // console.log('Up');
                                    break;

                                case DOWN:
                                    if (typeof elem.getSelectedItemData().DestinationId == 'undefined') {  //Country
                                        elem.val(elem.getSelectedItemData().CountryName);
                                    } else if (typeof elem.getSelectedItemData().PropertyId == 'undefined') { //Destination
                                        elem.val(elem.getSelectedItemData().CityName);
                                    } else { // Property
                                        elem.val(elem.getSelectedItemData().Name);
                                    }
                                    // console.log('Down');
                                    break;

                                default:
                                    // console.log('Other character (not an arrow key)');
                                    break;
                            }


                        } else {
                            if (typeof elem.getSelectedItemData().DestinationId == 'undefined') {  //Country
                                elem.val(elem.getSelectedItemData().CountryName);
                            } else if (typeof elem.getSelectedItemData().PropertyId == 'undefined') { //Destination
                                elem.val(elem.getSelectedItemData().CityName);
                            } else { // Property
                                elem.val(elem.getSelectedItemData().Name);
                            }
                        }


                        

                        
                        temponselect = elem.getSelectedItemData();
                      
                    },
                    onMouseOverEvent: function (e) {
                       // console.log(temponchoose);
                        if (Object.keys(temponchoose).length === 0 && temponchoose.constructor === Object) {

                            if (typeof elem.getSelectedItemData().DestinationId == 'undefined') {  //Country
                                elem.val(elem.getSelectedItemData().CountryName);
                            } else if (typeof elem.getSelectedItemData().PropertyId == 'undefined') { //Destination
                                elem.val(elem.getSelectedItemData().CityName);
                            } else { // Property
                                elem.val(elem.getSelectedItemData().Name);
                            }
                        } else {
                         //   console.log("mouser over");
                            //temponchoose = {};
                        }

                    },
                    onChooseEvent: function (e) {

                        temponchoose = elem.getSelectedItemData();
                        if (typeof elem.getSelectedItemData().DestinationId == 'undefined') {  //Country

                           // console.log(elem.getSelectedItemData().Cities.length);
                            if (elem.getSelectedItemData().Cities.length == 1) { // 1 Conutry 1 City 1 Properties => set default prop
                                if (elem.getSelectedItemData().Cities[0].Properties.length == 1) {
                                //    console.log(elem.getSelectedItemData().Cities[0].Properties[0].Name);

                                    elem.val(elem.getSelectedItemData().Cities[0].Properties[0].Name);
                                    $("input[id^='txt-search']").val(elem.getSelectedItemData().Cities[0].Properties[0].Name);
                                    _(booking).notifyProp("CountryId", 0);
                                    _(booking).notifyProp("DestinationId", 0);
                                    _(booking).notifyProp("PropertyId", elem.getSelectedItemData().Cities[0].Properties[0].PropertyId);
                                }
                            } else {

                                elem.val(elem.getSelectedItemData().CountryName);
                                $("input[id^='txt-search']").val(elem.getSelectedItemData().CountryName);
                                _(booking).notifyProp("CountryId", elem.getSelectedItemData().CountryID);
                                _(booking).notifyProp("DestinationId", 0);
                                _(booking).notifyProp("PropertyId", 0);
                                booking.Property = {};

                            }


                          
                        } else if (typeof elem.getSelectedItemData().PropertyId == 'undefined') { //Destination

                             
                            if (elem.getSelectedItemData().Properties.length == 1) { //1 City 1 Properties => set default prop
                               // console.log(elem.getSelectedItemData().Properties[0].Name);

                                elem.val(elem.getSelectedItemData().Properties[0].Name);
                                $("input[id^='txt-search']").val(elem.getSelectedItemData().Properties[0].Name);
                                _(booking).notifyProp("CountryId", 0);
                                _(booking).notifyProp("DestinationId", 0);
                                _(booking).notifyProp("PropertyId", elem.getSelectedItemData().Properties[0].PropertyId);
                            }
                            else {

                                elem.val(elem.getSelectedItemData().CityName);
                                $("input[id^='txt-search']").val(elem.getSelectedItemData().CityName);
                                _(booking).notifyProp("CountryId", 0);
                                _(booking).notifyProp("DestinationId", elem.getSelectedItemData().DestinationId);
                                _(booking).notifyProp("PropertyId", 0);
                                booking.Property = {};

                            }

                        } else { // Property
                            elem.val(elem.getSelectedItemData().Name);
                            $("input[id^='txt-search']").val(elem.getSelectedItemData().Name);
                            _(booking).notifyProp("CountryId", 0);
                            _(booking).notifyProp("DestinationId", 0);
                            _(booking).notifyProp("PropertyId", elem.getSelectedItemData().PropertyId);
                        }

                        $('.alert-close').show();

                    },
                    onShowListEvent: function (e) {
                        if ($.browser.mozilla) {
                            var containner = document.getElementsByClassName('easy-autocomplete-container');

                            var nonselected = $.grep(containner, function (item) {
                                return item.id.indexOf(elem[0].id) == -1;
                            })[0];
                            $(nonselected).hide();

                            var selected = $.grep(containner, function (item) {
                                return item.id.indexOf(elem[0].id) >= 0;
                            })[0];
                            $(selected).show();

                           
                        }
                    },
                    onHideListEvent: function (e) {


                        //if (Object.keys(temponchoose).length === 0 && temponchoose.constructor === Object) {
                        //    elem.val("");
                        //} else {
                        //    console.log("choosen");
                        //   //temponchoose = {};
                        //}
                       
                        if ($.browser.mozilla) {
                            $("div.easy-autocomplete-container").hide();
                        }
                    },
                    showAnimation: {
                        type: "fade", //normal|slide|fade
                        time: 400,
                        callback: function () { }
                    }
                }

                };
                return options;
            }            


            $('#txt-search-main').easyAutocomplete(getSearchOption($('#txt-search-main')));
            $('#txt-search-mask').easyAutocomplete(getSearchOption($('#txt-search-mask')));
            
            $('.load-bar').hide();
            //if firefox
            if ($.browser.mozilla) {
                setTimeout(function () {
                    $("div.easy-autocomplete-container").hide();
                }, 1);
            }


            //set property page
            function setdefaultproperty(elem) {

                elem.val(function () {
                    //console.log("set default prop : " + propertyId);
                    var getprop = $.grep(booking._setting.PropertyFilter, function (propdestitem) {
                        if (typeof propdestitem.PropertyId !== 'undefined') {
                           // console.log(propdestitem.PropertyId);
                            return propdestitem.PropertyId == propertyId;
                        } else return "";

                    })[0];
                   // console.log("getprop : " + getprop);
                    return getprop.Name;
                });
            }
            if (propertyId > 0) {

                setdefaultproperty($("#txt-search-main"));
                setdefaultproperty($("#txt-search-mask"));
                _(booking).notifyProp("PropertyId", propertyId);
            }

            

          /*  hoteldatepicker**/  
            var ArrShortMonth = ['Jan',
            'Feb',
            'Mar',
