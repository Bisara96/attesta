<script type="text/javascript">
    var $OnyxReward = null;
    var onrData = '';
    //$(".reservations").hover(function () {
    //    $(this).addClass('open');
    //})
    //.mouseover(function () {
    //    $(this).removeClass('open');
    //});


    $(document).ready(function(){
        $('.dropdown-menu').click(function(e){
            e.stopPropagation();
        });
       
        $(window).keydown(function (event) {
            if (event.keyCode == 13) {
                event.preventDefault();
                return false;
            }
        });
       
    });
</script>

<script type="text/javascript">
    //POST Method
    function isFacebookApp() {
        var ua = navigator.userAgent || navigator.vendor || window.opera;
        return (ua.indexOf("FBAN") > -1) || (ua.indexOf("FBAV") > -1);
    }

    var standalone = window.navigator.standalone,
        userAgent = window.navigator.userAgent.toLowerCase(),
        safari = /safari/.test(userAgent),
        ios = /iphone|ipod|ipad/.test(userAgent);
   
    var isIOSBrowser = false;
    var isUiwebview = false;
    if (ios) { // fix ios in-app browser problem
        if (!standalone && safari) {
            //browser
            isIOSBrowser = true;
        } else if (!standalone && !safari) {
            //uiwebview
            isUiwebview = true;
        };
    } 


    function haveQueryString(url) {

        if (url.indexOf('?') != -1)
            return true;
        return false

    }

    function validatepost() {

        if (($("#txt-search-main").val()== "" || $("#txt-search-mask").val() == "" )) {
            alert("Enter a destination or property");
            return false;
        } else

        if (booking.CountryId > 0 && booking.DestinationId == 0 && booking.PropertyId == 0) {
           // console.log("courtry selected");
            var countrytcheck = $.grep(booking._setting.PropertyFilter, function (checkitem) {
                if (typeof checkitem.CountryName !== 'undefined') {
                    return checkitem.CountryID == booking.CountryId;
                }
            })[0];
          //  console.log(countrytcheck);
            if ($("#txt-search-main").val() != countrytcheck.CountryName) {
                alert("Enter a destination or property");
                    return false;
                }

         } else if (booking.CountryId == 0 && booking.DestinationId > 0 && booking.PropertyId == 0) {
          //   console.log("destination selected");
             var destcheck = $.grep(booking._setting.PropertyFilter, function (checkitem) {
                 if (typeof checkitem.CityName !== 'undefined') {
                     return checkitem.DestinationId == booking.DestinationId;
                 }
             })[0];
             if ($("#txt-search-main").val() != destcheck.CityName) {
                 alert("Enter a destination or property");
                    return false;
                }

         } else if (booking.CountryId == 0 && booking.DestinationId == 0 && booking.PropertyId > 0) {
           //  console.log("property selected");
             var propcheck = $.grep(booking._setting.PropertyFilter, function (checkitem) {
                 if (typeof checkitem.PropertyId !== 'undefined') {
                     return checkitem.PropertyId == booking.Property.PropertyId;
                 }
            })[0];
             if ($("#txt-search-main").val().replace(/\s/g, '') !== propcheck.Name.replace(/\s/g, '')) {
                 alert("Enter a destination or property");
                 return false;
             }

        } else if (booking.PropertyId == 0 && booking.DestinationId == 0) {
                alert("Enter a destination or property");
                return false;
        }
       

        return true;

    }

    var objBookingLink = {};
    function postwith() { 

        //console.log($('#eac-container-txt-search-main ul li.selected .tab-space').val());
        //return false;

        if (validatepost()) {

            var json = JSON.stringify(booking);
            $("#hdfBooking").val(json);

            var urlToPostTo = 'https://gc.synxis.com';
            if ($OnyxReward) {
                urlToPostTo = 'https://gc.synxis.com/securelogin.aspx';
            }

            if (typeof getUrlParameter('integcert') !== 'undefined') {
                urlToPostTo = 'https://integcert.synxis.com/xbe/securelogin.aspx';
            }

            if ((booking.DestinationId > 0 && booking.PropertyId == 0) ||
                (booking.CountryId > 0 && booking.PropertyId == 0)) {
                urlToPostTo = '/bookingdestination.aspx';
            }

            var adidParam = "";
            if (typeof getUrlParameter('adid') !== 'undefined') {
                adidParam = getUrlParameter('adid');
                urlToPostTo = urlToPostTo + ((haveQueryString(urlToPostTo)) ? "&" : "?") + "adid=" + adidParam;
            }

            var parametersToPost = objBookingLink;
            console.log(JSON.stringify(parametersToPost));
            var myForm = document.createElement("form");
            myForm.method = "post";
            myForm.action = urlToPostTo;
            myForm.target = "_blank";
            if (ios) {
                myForm.target = "_self";
            }

            for (var parameter in parametersToPost) {
                var myInput = document.createElement("input");

                myInput.setAttribute("name", parameter);

                if (parameter == "Chain" && typeof getUrlParameter('integcert') !== 'undefined')
                    myInput.setAttribute("value", '1524645');
                else if (parameter == "hotel" && typeof getUrlParameter('integcert') !== 'undefined')
                    myInput.setAttribute("value", '95192');
                else if (parameter == "Shell" && typeof getUrlParameter('integcert') !== 'undefined')
                    myInput.setAttribute("value", 'AM');
                else if (parameter == "Template" && typeof getUrlParameter('integcert') !== 'undefined')
                    myInput.setAttribute("value", 'RSP');
                else
                    myInput.setAttribute("value", parametersToPost[parameter]);

                myForm.appendChild(myInput);
            }
            if (booking.Property.PropertyId == 4) {
                myForm.action = "http://www.boulevardbangkok.com/";
            }

            document.body.appendChild(myForm);
            myForm.submit();
            document.body.removeChild(myForm);


        }
    
      
    }
   

    function setBookingLink() {

            if (booking.Property.BookingType == 3 ||
            (booking.Property.BookingType == 2 && (booking.CheckOutDate - booking.CheckInDate).TotalDays > booking.Property.MinimumDayRequire) ||
            (booking.Property.BookingType == 1 && (booking.CheckOutDate - booking.CheckInDate).TotalDays < booking.Property.MinimumDayRequire)
            )
            {
               
                var getsitedomain = "amari.com"
                if (booking.Property.Propertyprefix.startsWith("A")) {
                    getsitedomain = "amari.com"
                }
                if (booking.Property.Propertyprefix.startsWith("W")) {
                    getsitedomain = "mosaic-collection.com"
                }
                if (booking.Property.Propertyprefix.startsWith("O")) {
                    getsitedomain = "ozohotels.com";
                }
                if (booking.Property.Propertyprefix.startsWith("S")) {
                    getsitedomain = "shama.com"
                }
                if (booking.Property.Propertyprefix.startsWith("ORB")) {
                    getsitedomain = "oriental-residence.com"
                }

                 
               
           var strEnquiry = "//" + 'www' + "." + getsitedomain + "/enquiries.aspx?hid=" + booking.PropertyId;
            if (booking.PromoCode !== "" && booking.PromoCode !== $booking.language.optional) {
                strEnquiry += "&promocode=" + booking.PromoCode;
            }

            if (booking.CheckOutString != null) {
                strEnquiry += "&checkindate=" + booking.CheckInString;
            }

            if (booking.CheckOutString != null) {
                strEnquiry += "&checkoutdate=" + booking.CheckOutString;
            }

            $('.linkCheckRate').attr('href', strEnquiry);
            $('.linkCheckRate').attr('target', '_blank');

        } else {

                var domainCurrency = 'USD';

                switch ('en') {
                 case "th":
                     domainCurrency = 'THB';
                     break;
                 case "de":
                     domainCurrency = 'EUR';
                     break;
                 case "ru":
                     domainCurrency = 'RUB';
                     break;
                 case "kr":
                     domainCurrency = 'KRW';
                     break;
                 case "cn":
                     domainCurrency = 'CNY';
                     break;
                 case "ru":
                     domainCurrency = 'RUB';
                     break;
                 default:
                     domainCurrency = booking.Property.DefaultCurrencyCode;
             }

            $('.linkCheckRate').attr('href', 'javascript:postwith();');
            objBookingLink = {
                hotel: (typeof booking.Property.SynXisHotelID == 'undefined' ? 0 : booking.Property.SynXisHotelID),
                Chain: '21906',
                Shell: booking.Property.SynXisShell,
                Template: booking.Property.SynXisTemplate,
                Dest: booking.Property.SynXisDestinations,
                Brand: booking.Property.SynXisBrands,
                Start: 'availresults',
                SortProductBy: 'Rate',
                locale: booking.LangCulture.replace('GB', 'US'),
                Arrive: $.datepicker.formatDate('yy-mm-dd', booking.CheckInDate),
                Depart: $.datepicker.formatDate('yy-mm-dd', booking.CheckOutDate),
                Rooms: booking.TotalRooms,
                Adult: booking.TotalAdult,
                Currency: domainCurrency
                //,Data: '201709052312000Hk>\xADG\x10t\x86\xF6|\xA8xB5\xD1xEA\xCD'
                //,Data: 'zXKnKYcilW1oyNb4Hk>\xADG\x10t\x86\xF6|\xA8xB5\xD1xEA\xCD'
                // ,Data: 'elhLbktZY2lsVzFveU5iNA=='
                //, Data: 'elhLbktZY2lsVzFveU5iNEhrPlx4QURHXHgxMHRceDg2XHhGNnxceEE4eEI1XHhEMXhFQVx4Q0Q='
            };

            if (typeof booking.Rooms[0] != 'undefined') {

                if (booking.Rooms[0].Children > 0) {
                    objBookingLink.Child = booking.Rooms[0].Children;
                    objBookingLink.ChildAges = booking.Rooms[0].ChildAges.join("|");
                   
                }
            }

            if (booking.PromoCode !== "" && booking.PromoCode !== $booking.language.optional) {
                objBookingLink.Promo = booking.PromoCode;
            }

            //temporary take out for Huahin promocode
            else { 
                // For CN storefront is not include 'AYG','ADQ' OKP
               
                if (booking.LangCulture.indexOf('CN') > 0 && (booking.PropertyId != 42 && booking.PropertyId != 91 && booking.PropertyId != 114)) {
                    objBookingLink.Promo = 'CNBestRate';
                }
            }

            if ($OnyxReward) {

                objBookingLink.Data = onrData;
                
            }

            if (typeof getUrlParameter('utm_source') !== 'undefined') {
                objBookingLink.utm_source = getUrlParameter('utm_source');
              
            }


            if (typeof getUrlParameter('utm_medium') !== 'undefined') {
                objBookingLink.utm_medium = getUrlParameter('utm_medium');
              
            }


            if (typeof getUrlParameter('utm_content') !== 'undefined') {
                objBookingLink.utm_content = getUrlParameter('utm_content');
               
            }


            if (typeof getUrlParameter('utm_campaign') !== 'undefined') {
                objBookingLink.utm_campaign = getUrlParameter('utm_campaign');
                
            }

            if (booking.DestinationId > 0 && booking.PropertyId == 0) {
                objBookingLink.DestinationId = booking.DestinationId;
                objBookingLink.TotalNights = booking.TotalNights;
            }

            if (booking.CountryId > 0 && booking.PropertyId == 0) {
                objBookingLink.CountryId = booking.CountryId;
                objBookingLink.TotalNights = booking.TotalNights;
            } 

            //var strBookingLink = "javascript:postwith('https://gc.synxis.com', " + JSON.stringify(objBookingLink) + ")";
            //$('.linkCheckRate').attr('href', strBookingLink);

         }

    }

    function isValidDate(d) {
        if (typeof d !== 'undefined') {
            var m = d.match(/^(\d{4})-(0?[1-9]|1[012])-(0?[1-9]|1[0-9]|2[0-9]|3[01])$/); //2019-02-03 format
            return (m);
        } else return false;
       
    }

    function setTextDate(d) {
        $('#txtCheckIn').datepicker("setDate", new Date(d));
        try {
            $('#txtMainCheckIn').datepicker("setDate", new Date(d));
        } catch (e) { }

    }


    function setCustomQueryString() {
        if (isValidDate(getUrlParameter('checkindate')))
        {
            _(booking).notifyProp("CheckInDate", new Date(getUrlParameter('checkindate')));

            setCookie("cindate", booking.CheckInDate, 365);
            setTextDate(getUrlParameter('checkindate'));
        } else {
            //if wrong parameter => get from cookies
            if (getCookie("cindate") != "") {
                _(booking).notifyProp("CheckInDate", new Date(getCookie("cindate")));
                setTextDate(getCookie("cindate"));
            } else {
                //if there is on cookie => get from new recent date
                _(booking).notifyProp("CheckInDate", new Date());
                setTextDate();
            }
        }

        if (typeof getUrlParameter('totalnights') !== 'undefined') {
            //_(booking).notifyProp("TotalNights", parseInt(getUrlParameter('totalnights')));
            _(booking).notifyProp("CheckOutDate", $booking.dateAdd(new Date(getUrlParameter('checkindate')), parseInt(getUrlParameter('totalnights'))));
            setCookie("coutdate", booking.CheckOutDate, 365);
          
           
            $('#txtCheckOut').datepicker("setDate", $booking.dateAdd(new Date(getUrlParameter('checkindate')), parseInt(getUrlParameter('totalnights'))));
            try {
                $('#txtMainCheckOut').datepicker("setDate", $booking.dateAdd(new Date(getUrlParameter('checkindate')), parseInt(getUrlParameter('totalnights'))));
            } catch (e) { }
        } else {

            if (isValidDate(getUrlParameter('checkoutdate'))) {
                _(booking).notifyProp("CheckOutDate", new Date(getUrlParameter('checkoutdate')));
                setCookie("coutdate", booking.CheckOutDate, 365);

                setTextDate(getUrlParameter('checkoutdate'));
               
            } else {
                if (getCookie("coutdate") != "") {
                    _(booking).notifyProp("CheckOutDate", new Date(getCookie("coutdate")));
                }
            }
        }


        if (typeof getUrlParameter('utm_source') !== 'undefined' && typeof getUrlParameter('promocode') !== 'undefined') {
            if ((getUrlParameter('utm_source').toLowerCase() == 'tripadvisor') && ((getUrlParameter('promocode').toLowerCase() == 'bookdirect'))) {

                sessionStorage.setItem("cpromocode", getUrlParameter('promocode'))
                sessionStorage.setItem("cutm_source", getUrlParameter('utm_source'))

            }
        } 

        if (sessionStorage.getItem("cpromocode") == "bookdirect" && sessionStorage.getItem("cutm_source") == "tripadvisor") {
            booking.PromoCode = "bookdirect";
            booking.PromoCodeAlias = "bookdirect";
            $('.ta-callout').show();
        }

    }

    function GetSelected(item) {
     //   console.log(item.Name);
    };

    function triggerFocus(elem) {

        var e = jQuery.Event("keyup", { keyCode: 65, which: 65 });
        $(elem).focus();
        if (booking.PropertyId == 0 && booking.DestinationId == 0) {
            $(elem).attr('value', '');
        }
        $(elem).triggerHandler(e);
        $(elem).trigger('change');

        $(elem).focusout(function () {
            if (Object.keys(temponchoose).length === 0 && temponchoose.constructor === Object) {
                $(elem).val("");
            }
        });

    }

    function clearInput(elem) {
        elem.val("");
        $('.alert-close').hide();
        elem.click();
    }

    function setLargeDate(dayname, daynum, month, year, type) {
        $('.big-date-' + type + 'daynum-bookingmask').text(daynum);
        $('.big-date-' + type + 'dayname-bookingmask').text(dayname);
        $('.big-date-' + type + 'month-bookingmask').text(month);
        $('.big-date-' + type + 'year-bookingmask').text(year);
       
    }

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function delete_cookie(name) {
        document.cookie = name + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
    }

  

</script>

