<script type="text/javascript">
    function showbooking() {
        $('.dropdown-menu-extend').show();
    }
    $('.close').click(function () {
        $('.dropdown-menu-extend').hide();
    });

    $('.close-member-rate').click(function () {
        $('.dropdown-menu-member-rate').hide();
        sessionStorage.setItem("CloseMemberRate", true);
    });

    $(document).ready(function(){
        if (sessionStorage.CloseMemberRate ||  getQuerystring('promocode') != "") {
            $('.dropdown-menu-member-rate').hide();
        }
    });

</script>
