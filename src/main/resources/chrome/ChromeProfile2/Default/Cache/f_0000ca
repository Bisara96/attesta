        }


        input.value = ArrLongDayName[today.getDay()] + ' ' + (today.getDate() < 10 ? '0' + today.getDate() : today.getDate()) + ' ' + ArrShortMonth[today.getMonth()] + ' ' + today.getFullYear() + ' - ' +
            ArrLongDayName[tomorrow.getDay()] + ' ' + (tomorrow.getDate() < 10 ? '0' + tomorrow.getDate() : tomorrow.getDate()) + ' ' + ArrShortMonth[tomorrow.getMonth()] + ' ' + tomorrow.getFullYear();


        inputmask.value = ArrLongDayName[today.getDay()] + ' ' + (today.getDate() < 10 ? '0' + today.getDate() : today.getDate()) + ' ' + ArrShortMonth[today.getMonth()] + ' ' + today.getFullYear() + ' - ' +
            ArrLongDayName[tomorrow.getDay()] + ' ' + (tomorrow.getDate() < 10 ? '0' + tomorrow.getDate() : tomorrow.getDate()) + ' ' + ArrShortMonth[tomorrow.getMonth()] + ' ' + tomorrow.getFullYear();


        invalue = input.value.toString().split('-')[0].split(' ');
        outvalue = input.value.toString().split('-')[1].trim().split(' ');

        setLargeDate(invalue[0], invalue[1], invalue[2], invalue[3], 'in');
        setLargeDate(outvalue[0], outvalue[1], outvalue[2], outvalue[3], 'out');


        var datepicker = new HotelDatepicker(document.getElementById('input-id'), {
            format: 'dddd DD MMM YYYY',
            i18n: {
                selected: 'Your Stay',
                night: 'Night',
                nights: 'nights',
                button: 'x',
                'checkin-disabled': 'Check-in disabled',
                'checkout-disabled': 'Check-out disabled',
                'day-names-short': ArrShortDayName,
                'day-names': ArrLongDayName,
                'month-names-short': ArrShortMonth,
                'month-names': AssLongMonth,
                'error-more': 'Date range should not be more than 1 night',
                'error-more-plural': 'Date range should not be more than %d nights',
                'error-less': 'Date range should not be less than 1 night',
                'error-less-plural': 'Date range should not be less than %d nights',
                'info-more': 'Please select a date range of at least 1 night',
                'info-more-plural': 'Please select a date range of at least %d nights',
                'info-range': 'Please select a date range between %d and %d nights',
                'info-default': 'Please select a date range',
                'check-in-text': 'Check-In:',
                'check-out-text': 'Check-Out:',
                'calendar-text': 'Calendar'
            },
            onSelectRange: function () {

                _(booking).notifyProp("CheckInDate", new Date(datepicker.start));
                _(booking).notifyProp("CheckOutDate", new Date(datepicker.end));
                inputmask.value = input.value;
                setCookie("cindate", booking.CheckInDate, 365);
                setCookie("coutdate", booking.CheckOutDate, 365);

                $('#scrollUp').show();

            }, onOpenDatepicker: function () {
                $('#scrollUp').hide();
                $('.overlay-booking').show();
                $('.big-date-checkout-bookingmask').addClass('opacity');
                //
            }
        });

        var datepickermask = new HotelDatepicker(document.getElementById('input-id-mask'), {
            format: 'dddd DD MMM YYYY',
            i18n: {
                selected: 'Your Stay',
                night: 'Night',
                nights: 'nights',
                button: 'x',
                'checkin-disabled': 'Check-in disabled',
                'checkout-disabled': 'Check-out disabled',
                'day-names-short': ArrShortDayName,
                'day-names': ArrLongDayName,
                'month-names-short': ArrShortMonth,
                'month-names': AssLongMonth,
                'error-more': 'Date range should not be more than 1 night',
                'error-more-plural': 'Date range should not be more than %d nights',
                'error-less': 'Date range should not be less than 1 night',
                'error-less-plural': 'Date range should not be less than %d nights',
                'info-more': 'Please select a date range of at least 1 night',
                'info-more-plural': 'Please select a date range of at least %d nights',
                'info-range': 'Please select a date range between %d and %d nights',
                'info-default': 'Please select a date range',
                'check-in-text': 'Check-In:',
                'check-out-text': 'Check-Out:',
                'calendar-text': 'Calendar'
            },
            onSelectRange: function () {

                _(booking).notifyProp("CheckInDate", new Date(datepickermask.start));
                _(booking).notifyProp("CheckOutDate", new Date(datepickermask.end));

                $('.navbar-fixed-top').show();
                $('#scrollUp').show();

                input.value = inputmask.value;
                setCookie("cindate", booking.CheckInDate, 365);
                setCookie("coutdate", booking.CheckOutDate, 365);

            }, onOpenDatepicker: function () {
                if ($(window).width() <= 480 && $(window).height() <= 923 ||  
                    $(window).width() <= 736 && $(window).height() <= 414 ||
                    $(window).width() <= 823 && $(window).height() <= 411 ||
                    navigator.userAgent.match(/Android/i)
                    || navigator.userAgent.match(/webOS/i)) {
                    $('.navbar-fixed-top').hide();
                }
                $('#scrollUp').hide();
                $('.overlay-booking').show();                
                $('.big-date-checkout-bookingmask').addClass('opacity');
            }
         });






        try {

            var open_datepicker_button = document.getElementById('big-date-inout');
            open_datepicker_button.addEventListener('click', open_datepicker_function);
            function open_datepicker_function() {
                datepicker.open();
            }

            var open_datepicker_button_mask = document.getElementById('big-date-inout-mask');
            open_datepicker_button_mask.addEventListener('click', open_datepicker_function_mask);
            function open_datepicker_function_mask() {
                datepickermask.open();
                $('.fadeMe').show();
            }
            inputmask.addEventListener('afterClose', function () {
                $('.navbar-fixed-top').show();
                $('#scrollUp').show();
            }, false);
        } catch (e) {

        }


        /** end hoteldatepicker **/

        
        //serialize to backend
        $(".BookingMaskCheckRate").click(function(e) {

                if (booking.CountryId == 0 && booking.DestinationId == 0 && booking.PropertyId == 0 && $("input[id^='txt-search']").val() == null) {
                alert("Enter a destination or property");
                return false;
            }
            var checkChildAges = true;
            var allAdult = 0;
            var allChilden = 0;
          
            _(booking.Rooms).each(function(room) {
                _(room.ChildAges).each(function(childAge) {
                    if (childAge == 0) {
                        checkChildAges = false;
                    }
                });

                allAdult += room.Adults; 
                allChilden += room.Children;

            });
            if (checkChildAges == false) {
                alert("Please select child age(s)");
                return false;
            }


            setBookingLink();

            //push data here

            var bookingPosition = 'main_mask' ;
            var bookingPositionStr = $(this).parents().attr('id').toString();
            if (bookingPositionStr.toLowerCase().indexOf("header") >= 0)
                bookingPosition = 'header_mask';
            

            dataLayer.push({
                'event':'trackEvent',
                'eventDetails.category': 'check_availability', 
                'eventDetails.action': 'click', 
                'eventDetails.label': bookingPosition, 
                'check_property_name': booking.Property.NameEN, 
                'check_rooms':booking.TotalRooms, 
                'check_adults':allAdult, 
                'check_children':allChilden, 
                'check_days_staying':booking.TotalNights ,
                'check_days_advance': $booking.dateDiff(booking.CheckInDate, $booking.today),
                'bookingHotelCode': booking.Property.Propertyprefix
            });
           
            dataLayer.push({
                'event':'productAdd',
                'ecommerce': {
                    'currencyCode': booking.Property.DefaultCurrencyCode,
                    'add': { 
                        'products': [
                          {
                              'name': booking.Property.NameEN,  
                              'category':'Daily_Rate',  
                              'brand': 'Amari',  
                              'quantity': booking.TotalRooms * booking.TotalNights
                             
                          }]
                    }
                }

            });
            
            return true;
        });

        $("form").submit(function (e) {
            setBookingLink();
            //TODO ReStructure
            _(booking.Rooms).each(function(room) {
                _(room).notifyProp("ChildrenAge", room.ChildAges.toString());
            });

            booking.PromoCode = $(".BookingMaskPromoCode").val();
            booking.PromoCodeAlias = $(".BookingMaskPromoCode").val();

            if (booking.PromoCode == $booking.language.optional)
                booking.PromoCode = "";
            if (booking.PromoCodeAlias == $booking.language.optional)
                booking.PromoCodeAlias = "";

            booking.CheckInString = $.datepicker.formatDate('yy-mm-dd', booking.CheckInDate);
            booking.CheckOutString = $.datepicker.formatDate('yy-mm-dd', booking.CheckOutDate);

            delete booking.CheckInDate;
            delete booking.CheckOutDate;

            var json = JSON.stringify(booking);
            $("#hdfBooking").val(json);
            $("#hdfMainBooking").val(json);
        });

        if (getQuerystring('promocode') == "") {
            $(".BookingMaskPromoCode").val($booking.language.optional);
        }
        else {
            $(".BookingMaskPromoCode").val(getQuerystring('promocode'));
            booking.PromoCode = $(".BookingMaskPromoCode").val();
            booking.PromoCodeAlias = $(".BookingMaskPromoCode").val();
            $(".BookingMaskPromoCode").removeClass('gray');
        }
    });
    // ]]>

</script>


